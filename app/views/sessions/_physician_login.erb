<h1> Physician Log In</h1>

<%= form_for :session, url: create_session_path(:ph => @ph) do |f| %>
	<div class="field">
		      <%= f.label :email %>
		      <%= f.email_field :email, class: 'form-control' %>

		      <%= f.label :password %>
		      <%= f.password_field :password, class: 'form-control' %>

		      <%= f.submit "Log In", class: "btn btn-primary" %>
	</div>
<% end %>
